# API Configuration
VITE_API_URL=http://localhost:5000/api

# App Configuration
VITE_APP_NAME=Te_QuickRide
VITE_APP_URL=http://localhost:3000

# Google Maps API (if needed for tracking)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# OAuth Client IDs (for frontend OAuth)
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_FACEBOOK_APP_ID=your-facebook-app-id

# Feature Flags
VITE_ENABLE_LOYALTY_PROGRAM=true
VITE_ENABLE_REVIEWS=true
VITE_ENABLE_TRACKING=false

# Payment Gateway (for frontend integration)
VITE_VNPAY_ENABLED=true
VITE_MOMO_ENABLED=true
VITE_ZALOPAY_ENABLED=true

# Cloudinary (if using for image uploads from frontend)
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
VITE_CLOUDINARY_UPLOAD_PRESET=your-upload-preset
